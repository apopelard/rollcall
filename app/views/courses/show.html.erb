<div class="page-header">
  <h1>Course #<%= @course.id %></h1>
</div>

<p>
  <b>Department:</b>
  <%= @course.department %>
</p>

<p>
  <b>Number:</b>
  <%= @course.number %>
</p>

<p>
  <b>Title:</b>
  <%= @course.title %>
</p>

<p>
  <b>Meetings:</b>
  <ul>
    <% @meetings.each do |meeting| %>
    <li>
      <%= link_to meeting.held_on, meeting_url(meeting.id) %>
    </li>
    <% end %>
  </ul>
</p>


